<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<Button android:layout_height="wrap_content" android:id="@+id/encryptbutton"
		android:text="Encrypt Your File" android:layout_width="wrap_content"
		android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
		android:layout_marginTop="143dp"></Button>
	<Button android:text="Next" android:id="@+id/encryptnxtact"
		android:layout_height="wrap_content" android:layout_width="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_alignParentRight="true" android:layout_toRightOf="@+id/encryptbutton"></Button>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/keystextview"
		android:text="Encrypted_keys" android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_centerVertical="true" android:layout_centerHorizontal="true"></TextView>


</RelativeLayout>
